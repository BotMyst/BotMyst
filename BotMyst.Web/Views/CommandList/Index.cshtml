@model BotMystGuild

<section id="commandList">
    @if (Model.HasBotMyst == false)
    {
        <!-- TODO: Add a proper invite link. -->
        <p>BotMyst isn't on the <strong>@Model.Guild.Name</strong> server. You can add it by visiting <a href="#">this link</a>.</p>
    }
    else
    {
        @await Html.PartialAsync("CurrentGuildBar", Model.Guild)

        <h6>Available commands:</h6>
    }
</section>
